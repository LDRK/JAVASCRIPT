ul{
list-style: none;
}
#opciones>li{
float: left;
text-align: center;
}
#opciones>li>ul{
display: none;
}
#opciones li>a{
background-color: grey;
color: white;
display: block;
text-decoration: none;
padding: 10px;
min-width: 110px;
}
#opciones li>a:hover{
color: #000;
background-color: #eaeaea;
}
#opciones>li:hover>ul {
display:block;
}
#pie{
background-color:#ebebeb ;
height: 30px;

